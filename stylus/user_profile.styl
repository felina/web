borderValue = 4px
profileBorder_height = 100px
profileBorder_width = 100px
profilePic_height = profileBorder_height - 2 * borderValue
profilePic_width = profileBorder_width - 2 * borderValue
namePad = profileBorder_height * 0.2

table
    border: 1px

td, tr
    border: 0px


.profile_pic
    background-color: rgb(255,255,255)
    border: borderValue solid rgb(255,255,255)
    border-radius: 2px 2px 2px 2px
    height: profileBorder_height
    width: profileBorder_width
    img
        height: profilePic_height
        width: profilePic_width
        border: 0px none

.alternate_name
    color: rgb(255, 255, 255)
    text-rendering: optimizelegibility
    text-shadow: 0px 0px 3px rgba(0, 0, 0, 0.8)
    font-size: 250%
    padding-top: namePad

.title_sub
    color: rgb(255, 255, 255)
    text-rendering: optimizelegibility
    text-shadow: 0px 0px 3px rgba(0, 0, 0, 0.8)
    font-size: 100%

.title_head
    color: rgb(255, 255, 255)
    text-rendering: optimizelegibility
    text-shadow: 0px 0px 3px rgba(0, 0, 0, 0.8)
    font-size: 100%

.photo_gallery
    cellspacing: 0
    cellpadding: 0

.photo_ind
    height: auto
    //height: 100% //Get back to work in FF
    //width: 100%
    max-width: 100%

.photo1
    width: 100%

.picbody
    width: 100%

.newsfeed_photo
    margin-right: 10px
    height: 70px
    width: auto
    float: left

.newsfeed_users
    float: left

.badge_ind
    height: auto
    max-width: 100%
    //height: 100%

.gallery_wrap
    border: 1px solid black

.badge_wrap
    border: 1px solid black

.newsfeed_list
    list-style-type: none

.hr_style
     border: 0;
     height: 1px
     background-image: -webkit-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))
     background-image: -moz-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))
     background-image: -ms-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))
     background-image: -o-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))
     width: 90%
     align: center


.vertical_style
    border-right: 1px solid #8d8d8d

.table_background
    background-color: #ffffff

.body_background
    background-color: #dbdbdb

.cover_image
    background-size: cover
    background-repeat: no-repeat
    background-position: 0 center
    height: 350px

.cover_properties
    padding-top: 120px

.navbar_style
    padding-top: 10px

